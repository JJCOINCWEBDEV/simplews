SimpleWS=/$PWD filter="*.rb Rakefile *.sh *.rake *.txt" {
 README.rdoc
 Rakefile
 bin=bin{
  start_ws
  start_jobs_ws
 }
 lib=lib {
  simplews.rb
  rake_pipeline.rb
  simplews=simplews {
   jobs.rb
   rake.rb
  }
 }
 test=test {
  test_helper.rb
  test_simplews.rb
  simplews=simplews{
   test_jobs.rb
  }
 }
}
